module RoboCompMelexLogger
{
  struct LogMessage
  {
    string namespace;
    string sender;
    string method;
    string file;
    int line;
    string timeStamp;
    string message;
    string type;	
    string fullpath;
  };


  struct LogNamespace
 {
     string sender;
     string namespace;
     string headers;	     	
 };
  

  interface MelexLogger
  {
    void sendMessage(LogMessage m);
    void createNamespace (LogNamespace logn);	
  };
};

